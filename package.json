{
  "name": "ip2d",
  "version": "1.2.1",
  "main": "index.js",
  "author": "0xflotus",
  "repository": {
    "type": "git",
    "url": "https://github.com/0xflotus/ip2d.git"
  },
  "scripts": {
    "build": "tsc index.ts",
    "prepare": "tsc index.ts",
    "prepublishOnly": "yarn run test",
    "pretest": "yarn run lint",
    "test": "jest test.ts",
    "posttest": "yarn run size",
    "lint": "tslint index.ts",
    "size": "bundlesize --files index.js --max-size 1kb --compression none",
    "postsize": "yarn run sizeGzip",
    "sizeGzip": "bundlesize --files index.js --max-size 0.5kb --compression gzip",
    "postsizeGzip": "yarn run sizeBrotli",
    "sizeBrotli": "bundlesize --files index.js --max-size 0.5kb --compression brotli"
  },
  "license": "MIT",
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.1.4",
    "@size-limit/time": "11.1.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.2",
    "bundlesize": "0.18.2",
    "jest": "29.7.0",
    "tslint": "6.1.3",
    "typescript": "5.4.5",
    "yarn": "1.22.22"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 0
      }
    },
    "displayName": {
      "name": "ip2d-test",
      "color": "grey"
    }
  }
}
